
<%= form_for @user do |f| %>
<%= f.label :email, 'Email' %>
 <%= f.email_field :email %>
 <%= f.label :username %>
 <%= f.text_field :username %>
 <%= f.label :password, 'Password' %>
 <%= f.text_field :password %>
 <%= f.label :password_confirm, 'Password Confirmation'%>
 <%= f.text_field :password_confirm %>
 <%= f.submit "Sign Up!" %>

<% end%>